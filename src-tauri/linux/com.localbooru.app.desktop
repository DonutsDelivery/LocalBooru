[Desktop Entry]
Name=LocalBooru
Comment=Local image library with automatic AI tagging
GenericName=Image Library
Exec=localbooru %U
Icon=com.localbooru.app
Terminal=false
Type=Application
Categories=Graphics;Photography;Viewer;
Keywords=image;gallery;booru;tagger;ai;
MimeType=image/jpeg;image/png;image/gif;image/webp;image/bmp;image/tiff;
StartupNotify=true
StartupWMClass=LocalBooru
Actions=NewWindow;

[Desktop Action NewWindow]
Name=New Window
Exec=localbooru --new-window
